<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernate.dao.TSergEntity" table="t_serg" schema="wa1">
        <id name="sergId">
            <column name="serg_id" sql-type="int(11)"/>
        </id>
        <property name="sergtext">
            <column name="sergtext" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>